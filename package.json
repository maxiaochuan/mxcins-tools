{
  "name": "@mxcins/tools",
  "version": "3.1.0",
  "description": "tools",
  "homepage": "https://github.com/maxiaochuan/mxcins-tools",
  "bugs": {
    "url": "https://github.com/maxiaochuan/mxcins-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxiaochuan/mxcins-tools"
  },
  "license": "MIT",
  "files": [
    "lib",
    "bin",
    "src/types.d.ts"
  ],
  "types": "src/types.d.ts",
  "bin": {
    "mtools": "./bin/mxcins-tools.js",
    "mxcins-tools": "./bin/mxcins-tools.js"
  },
  "scripts": {
    "build": "babel src --out-dir lib -x .ts",
    "lint": "yarn lint:ts",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --fix --ext .js",
    "lint-staged:ts": "tslint --fix",
    "lint:ts": "tslint --fix \"src/**/*.ts*\"",
    "prepublishOnly": "yarn build && np --no-cleanup --yolo --no-publish",
    "prettier": "prettier --write \"./src/**/*\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx}": "yarn lint-staged:ts"
  },
  "dependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@babel/register": "7.4.0",
    "@types/prettier": "^1.16.1",
    "autoprefixer": "^9.5.0",
    "chokidar": "^2.1.5",
    "docz": "1.0.0-rc.8",
    "docz-theme-default": "1.0.0-rc.8",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^5.0.1",
    "less": "^3.9.0",
    "path-is-root": "^0.1.0",
    "prettier": "^1.16.4",
    "resolve-bin": "^0.4.0",
    "rimraf": "^2.6.3",
    "rollup": "1.12.1",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.3.3",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-typescript2": "0.20.1",
    "signale": "^1.4.0",
    "slash2": "^2.0.0",
    "sort-package-json": "^1.22.1",
    "temp-dir": "^1.0.0",
    "through2": "^3.0.1",
    "typescript": "^3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@types/assert": "^1.4.2",
    "@types/gulp": "^4.0.6",
    "@types/rimraf": "^2.0.2",
    "@types/signale": "^1.2.1",
    "@types/through2": "^2.0.34",
    "assert": "^1.4.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "np": "^4.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "yargs-parser": "^13.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "XiaochuanMa <mxcins@gmail.com> (https://github.com/maxiaochuan)"
  ]
}
