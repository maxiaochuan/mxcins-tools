{
  "name": "@mxcins/library",
  "version": "0.0.0-alpha.0",
  "description": "tools",
  "homepage": "https://github.com/maxiaochuan/mxcins-tools",
  "bugs": {
    "url": "https://github.com/maxiaochuan/mxcins-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxiaochuan/mxcins-tools"
  },
  "license": "MIT",
  "types": "src/types.d.ts",
  "bin": {
    "mts": "./bin/mts.js"
  },
  "files": [
    "lib",
    "bin",
    "src/types.d.ts"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc",
    "dev": "rm -rf lib && tsc --watch",
    "lint": "eslint --fix '**/*.{js,ts}'",
    "prepublishOnly": "yarn build && np --no-cleanup --yolo --no-publish",
    "prettier": "prettier --write '**/*.{js,ts,jsx,tsx,less,md,json}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "pretty-quick --staged"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "np": "6.2.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "typescript": "3.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "XiaochuanMa <mxcins@gmail.com> (https://github.com/maxiaochuan)"
  ]
}
